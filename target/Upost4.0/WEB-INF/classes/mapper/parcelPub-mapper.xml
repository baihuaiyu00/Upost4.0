<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.xupt.mapper.ParcelMapper">
    <!-- 添加个人办公机账号 -->
    <insert id="parcelPub" parameterType="Parcel">
        insert into parcel_inform (awb,set_stu_id,location_1,other_1,location_2,other_2,type,company,reward,create_time,size,weight,par_status)
                         VALUES (#{awb},#{set_stu_id},#{location_1},#{other_1},#{location_2},#{other_2},#{type},#{company},#{reward},#{create_time},#{size},#{weight},#{par_status});
    </insert>
</mapper>

