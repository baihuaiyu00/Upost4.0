<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.xupt.mapper.UserMapper">
    <select id="userInfoGet" parameterType="java.lang.String" resultType="User">
        SELECT stu_num,sex,stu_phone,email,stu_passwd,stu_name,school_id,major,stu_nickname,head,signature,credibility,set_order,get_order FROM stu_login WHERE stu_num = #{stu_num};
    </select>
</mapper>

