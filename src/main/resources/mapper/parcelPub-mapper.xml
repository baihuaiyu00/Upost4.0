<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.xupt.mapper.UserMapper">
    <!-- 添加个人办公机账号 -->
    <insert id="parcelPub" parameterType="Parcel">
        insert into parcel_inform (set_stu_id,location_1,location_2,type,company,reward,create_time,deadline,size,weight,par_status) VALUES (#{set_stu_id},#{location_1},#{location_2},#{type},#{company},#{reward},#{create_time},#{deadline},#{size},#{weight},#{par_status});
    </insert>
</mapper>

